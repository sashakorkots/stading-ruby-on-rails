<h1>Questions</h1>

<% @questions.each do |question| %>
    <h2><%= question.title %></h1>    
    <p>
        <%= question.body %>
        <br>
        <%= link_to "Show", question_path(question)%> |
        <%= link_to "Edit", edit_question_path(question)%> |
        <%= link_to "Delete", question_path(question), data: {method: :delete, confirm: "Are you sure?"} %>
    </p>
    <time><%= question.created_at %></time>
<% end %>